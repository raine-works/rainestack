{
	"name": "rainestack",
	"private": true,
	"scripts": {
		"build": "turbo run build",
		"clean": "sudo find . -name \"node_modules\" -type d -prune -exec rm -rf {} +",
		"db:dev": "turbo run db:dev",
		"db:deploy": "turbo run db:deploy",
		"db:start": "docker compose -f ./docker/dev.yaml up -d --force-recreate",
		"dev": "turbo run dev",
		"format": "biome format --write .",
		"lint": "turbo run lint",
		"typecheck": "turbo run typecheck"
	},
	"workspaces": {
		"packages": [
			"packages/*"
		],
