{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "React Library",
	"extends": "./base.json",
	"compilerOptions": {
		"lib": ["ES2020", "DOM", "DOM.Iterable"],
		"jsx": "react-jsx",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noFallthroughCasesInSwitch": true,
		"types": ["bun-types"]
	}
}
